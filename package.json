{
  "name": "ponyracer",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run --coverage",
    "test:e2e": "vite build && start-server-and-test preview http://localhost:4173/ 'cypress open --e2e'",
    "test:e2e:ci": "vite build && start-server-and-test preview http://localhost:4173/ 'cypress run --e2e --browser chrome'",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "vue-ninja": "vue-ninja"
  },
  "dependencies": {
    "bootstrap": "5.2.0",
    "font-awesome": "4.7.0",
    "vue": "3.2.38"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "1.1.4",
    "@types/jsdom": "20.0.0",
    "@vitejs/plugin-vue": "3.1.0",
    "@vitest/coverage-istanbul": "0.23.1",
    "@vue/eslint-config-prettier": "7.0.0",
    "@vue/eslint-config-typescript": "11.0.0",
    "@vue/test-utils": "2.0.2",
    "@vue/tsconfig": "0.1.3",
    "cypress": "10.7.0",
    "eslint": "8.23.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-vue": "9.4.0",
    "jsdom": "20.0.0",
    "mochawesome": "7.1.3",
    "mochawesome-merge": "4.2.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "start-server-and-test": "1.14.0",
    "typescript": "4.8.2",
    "vite": "3.1.0",
    "vitest": "0.23.1",
    "vue-ninja": "0.4.0",
    "vue-tsc": "1.0.0-alpha.3"
  }
}
